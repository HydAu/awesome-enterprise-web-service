{
  "name": "@kkemple/awesome-enterprise-web-service",
  "version": "0.0.1",
  "description": "An enterprise grade web service example",
  "main": "lib/index.js",
  "scripts": {
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "migration:create": "sequelize migration:create",
    "seed:create": "sequelize seed:create",
    "serve": "nodemon",
    "lint": "eslint .",
    "vulnerabilities": "nsp check",
    "start": "node lib/index.js",
    "compile": "rimraf lib && babel src --out-dir lib --source-maps inline",
    "pretest": "npm run lint",
    "test": "npm run db:migrate && npm run db:seed:all && istanbul cover tape -- -r babel-register \"src/**/*.test.js\"",
    "test:coverage": "npm run test && codeclimate-test-reporter < coverage/lcov.info",
    "pretest:unit": "npm run lint",
    "test:unit": "tape -r babel-register \"src/**/unit.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/awesome-enterprise-web-service.git"
  },
  "keywords": [],
  "author": "Kurtis Kemple <kurtiskemple@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awesome-enterprise-web-service/issues"
  },
  "homepage": "https://github.com/awesome-enterprise-web-service#readme",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "catbox-redis": "^1.0.10",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "hapi": "^13.3.0",
    "hapi-auth-basic": "^4.1.0",
    "hapi-auth-jwt2": "^5.8.0",
    "hapi-statsd": "^5.0.0",
    "hapi-swagger": "^5.0.1",
    "inert": "^3.2.0",
    "joi": "^8.0.5",
    "jsonwebtoken": "^5.7.0",
    "lodash.assign": "^4.0.8",
    "lodash.flattendeep": "^4.2.0",
    "lodash.omit": "^4.2.1",
    "lodash.reduce": "^4.3.0",
    "mysql": "^2.10.2",
    "newrelic": "^1.26.2",
    "sequelize": "^3.21.0",
    "sequelize-cli": "^2.3.1",
    "socket.io": "^1.4.5",
    "socketio-jwt": "^4.3.4",
    "uuid": "^2.0.2",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "nock": "^8.0.0",
    "nodemon": "^1.9.1",
    "nsp": "^2.3.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "tape": "^4.5.1"
  }
}
